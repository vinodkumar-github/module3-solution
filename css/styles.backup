.navbar-default
      {background-color: rgb(93, 93, 93);border-color:#000000} /* background colour of heading*/

.navbar-default .navbar-brand
{color:#030303;; font-weight: bolder;   } /*color of words Food, LLC*/

.navbar-default .navbar-brand:focus,
.navbar-default .navbar-brand:hover
{color:#000000;background-color:transparent; font-weight: bolder; -webkit-text-stroke: 1px rgb(0, 0, 0);
  text-stroke: 1px black ;} /* color and size of words Food, LLC*/

.navbar-default .navbar-toggle
{border-color:#000 }

.navbar-default .navbar-toggle:focus,
.navbar-default .navbar-toggle:hover
{background-color:#ffffff} /* focus and hover state background colour of three stripes - also called icon-bar */


.navbar-default .navbar-toggle .icon-bar
{background-color:#000000} /* colour of three stripes - also called icon-bar */





.navbar-default .navbar-nav>.open>a { box-sizing:border-box ;color:#1d1d1d;background-color:#ffffff; border: 2px solid black;}
.navbar-default .navbar-nav>.open>a:focus,
.navbar-default .navbar-nav>.open>a:hover{color:#000;background-color:#e7e7e7;border: 5px solid black}


@media (min-width:768px) {.navbar-right {
  display: none;


} }

